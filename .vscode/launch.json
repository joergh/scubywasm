{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug scubywasm service runner",
      "type": "python",
      "request": "launch",
      "python": "${workspaceFolder}/tools/.venv/bin/python",
      "program": "${workspaceFolder}/tools/.venv/bin/scubywasm-service-runner",
      "cwd": "${workspaceFolder}",
      "args": ["/var/lib/scubywasm/engine.wasm", "/var/www/results", "/var/lib/scubywasm/scenarios.json"],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
